{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "dubulist",
  "version": "0.1.0",
  "identifier": "com.dubulist",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "dubulist",
        "width": 400,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/dubu.png",
      "icons/dubu.ico"
    ]
  }
}